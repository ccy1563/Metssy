<!DOCTYPE html>
<html>
  <head>
    <title>Yeetsy</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">

      <meta property="og:title" content="Yeetsy" />
     
     <meta property="og:description"
       content="Yeetsy is Etsy clone, an E-commerce website with a concentration hand-crafted artisan goods."/>
     <meta property="og:url"
       content="https://yeetzylol.herokuapp.com/#/"/>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <script type="text/javascript">
      window.cat_clothes = "<%= image_url('cat_clothes.jpg') %>";
      window.cat_decorations = "<%= image_url('cat_decorations.jpg') %>";
      window.cat_jewelry = "<%= image_url('cat_jewelry.jpg') %>";
      window.cat_sale = "<%= image_url('cat_sale.jpg') %>";
      window.cat_skincare = "<%= image_url('cat_skincare.png') %>";
      window.cat_halloween = "<%= image_url('cat_halloween.jpg') %>";

      window.clothesURL = "<%= image_url('clothes.jpg') %>";


      window.americanExpress = "<%= image_url('american_express.png') %>";
      window.discover = "<%= image_url('discover.jpg') %>";
      window.klarna = "<%= image_url('klarna.jpg') %>";
      window.mastercard = "<%= image_url('mastercard.png') %>";
      window.paypal = "<%= image_url('paypal.jpg') %>";
      window.visa = "<%= image_url('visa.jpg') %>";

      window.united_states = "<%= image_url('united_states.png') %>";

      window.hand = "<%= image_url('hand.png') %>";
      window.ticket = "<%= image_url('ticket.png') %>";

      window.leaf = "<%= image_url('leaf.png') %>";

      window.angellist = "<%= image_url('angellist.png') %>";
      window.github = "<%= image_url('github.png') %>";
      window.linkedin = "<%= image_url('linkedin.png') %>";
      
    </script>
      <link rel="shortcut icon" type='image/png' href="<%= image_url('favicon.png') %>" />
  </head>

  <body>
    <%= yield %>
  </body>
</html>